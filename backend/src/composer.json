  "scripts": {
    "start": "php -S localhost:8080",
    "env:init": "cp ./.env.example ./.env.dev && cp ./.env.example ./.env.prod",
    "env:prod": "cp ./.env.prod ./.env",
    "env:dev": "cp ./.env.dev ./.env",
    "key:make": "php ./scripts/make-key.php"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app"
    }
  },
  "require": {
    "firebase/php-jwt": "^5.2",
    "intervention/image": "^2.4",
    "slim/psr7": "^1.4",
    "slim/slim": "4.*",
    "vlucas/phpdotenv": "^5.3"
  }
